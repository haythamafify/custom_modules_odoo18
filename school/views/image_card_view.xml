<odoo>
    <data>
        <record id="view_image_card_form" model="ir.ui.view">
            <field name="name">image.card.form</field>
            <field name="model">image.card</field>
            <field name="arch" type="xml">
                <form string="Image Card">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="card_id"/>
                            <field name="image" widget="image"/>
                            <field name="receipt_image" widget="image"/>

                            <field name="text"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_image_card_list" model="ir.ui.view">
            <field name="name">image.card.list</field>
            <field name="model">image.card</field>
            <field name="arch" type="xml">
                <list string="Image Card">
                    <field name="name"/>
                      <field name="card_id"/>
                    <field name="image"/>
                     <field name="receipt_image" />
                    <field name="text"/>
                </list>
            </field>
        </record>

        <record id="action_image_card" model="ir.actions.act_window">
            <field name="name">Image Cards</field>
            <field name="res_model">image.card</field>
            <field name="view_mode">list,form</field>
        </record>

        <menuitem id="menu_image_card_root" name="Image Cards" parent="menu_student_root"/>
        <menuitem id="menu_image_card_root" name="Image Cards"/>
        <menuitem id="menu_image_card" name="Image Cards" parent="menu_image_card_root" action="action_image_card"/>
    </data>
</odoo>